{
  "$schema": "https://json-schema.axonivy.com/market/10.0.0/product.json",
  "installers": [
    {
      "id": "maven-dropins",
      "data": {
        "dependencies": [
          {
            "groupId": "com.axonivy.connector.openai",
            "artifactId": "openai-assistant",
            "version": "${version}"
          }
        ],
        "repositories": [
          {
            "id": "maven.axonivy.com",
            "url": "https://maven.axonivy.com",
            "snapshots": {
              "enabled": "true"
            }
          }
        ]
      }
    }
  ]
}